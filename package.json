    "axios": "^1.6.8",
    "editorconfig": "^2.0.0",
    "fs-utils": "^0.7.0",
    "jsqr": "^1.4.0",
    "jwt-decode": "^4.0.0",
    "luxon": "^3.4.4",
    "pinia": "^2.1.7",
    "vee-validate": "^4.12.4",
    "vue": "^3.3.11",
    "vue-i18n": "^9.13.1",
    "vue-jwt-decode": "^0.1.0",
    "vue-router": "^4.3.2",
